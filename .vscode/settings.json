{
  "editor.folding": false,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "es5",
  "prettier.printWidth": 100,
  "prettier.bracketSpacing": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.d.ts": { "when": "$(basename).ts" },
    "**/*.js": { "when": "$(basename).ts" }
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
